{
  "compilerOptions": {
    "paths": {
      "untangled-web": ["../../dist/index.js"],
      "untangled-web/*": ["../../dist/*"]
    },
    "types": ["../../dist/types"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "target": "es2016",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true
  }
}
